# EagerDevelopers Backend Environment Configuration
# Copy this file to .env and update the values as needed

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-please-make-it-at-least-32-characters-long
JWT_EXPIRES_IN=30m

# Database Configuration  
DATABASE_URL="postgresql://username:password@localhost:5432/eagerdevelopers?schema=public"

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Server Configuration
PORT=3000
NODE_ENV=development

# Optional: Additional Security
BCRYPT_ROUNDS=10
RATE_LIMIT_TTL=60000
RATE_LIMIT_MAX=10